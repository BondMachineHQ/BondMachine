; addition of two real numbers one passed as an argument and the other as an immediate value
; r0 is the real part of the argument
; {{ .Params.numberreal }} is the real part of the immediate value
%fragment addargrealnumreal template:true default_setop:rset resin:r0 resout:r0 
	{{ .Params.setop }}	r1, {{ .Params.numberreal }}
	{{ .Params.addop }}	r0, r1
%endfragment
